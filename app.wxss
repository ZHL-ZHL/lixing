/**app.wxss**/

@import '/miniprogram_npm/weui-miniprogram/weui-wxss/dist/style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  background: #f8f5f5;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.nav-content{
  margin-bottom:10rpx;
  width:100%;
  background:#fff;
}
.nav-title{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color:#363636;
  text-align: center;
  font-size:24rpx;
  margin:20rpx 0rpx;
}
.nav-line,.nav-text{
  display: inline-block;
  vertical-align: middle;
}
.nav-text{
  margin:0rpx 30rpx;
}
.nav-line{
  width:60rpx;
  height: 2rpx;
  background: #e4e3e3;
}
.nav-list-content{
  border-bottom:1rpx solid #e4e3e3;
  margin:0rpx 20rpx 30rpx 20rpx;
  padding:10rpx 0rpx 30rpx 0rpx;
}
.clearfix:after{
  content: "";
  display: block;
  height: 0;
  clear:both;
  visibility: hidden;
}
.marginright25{
  margin-right:30rpx
}
.fontsixe13{
  font-size: 26rpx;
}
.fontsixe10{
  font-size: 20rpx;
}
.fontsixe12{
  font-size: 24rpx;
}
.ellipsistext{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.widht70p{
  width:65%;
  display: inline-block;
  vertical-align: middle;
}
.colorccc{
  color:#999;
}
.colorred{
  color:red;
}
button.loadbtn{
  border-radius: 40rpx;
  height:68rpx;
  line-height: 68rpx;
  font-size: 24rpx;
  color:#666;
  margin:20rpx 20rpx 40rpx 20rpx;
  background-color: rgb(216, 216, 216);
}
button::after {
  border: none
}

.icon-start{
  display: inline-block;
  width: 30rpx;
  height:30rpx;
  vertical-align: middle;
}


button.getOader{
  width:35%;
  margin-top:30rpx;
  border-radius:40rpx;
  height:60rpx;
  line-height: 60rpx;
  background: rgb(255, 102, 0);
  color: #fff;
  font-size: 28rpx;
}
button.getOader:hover,button.getOader:active{
  background: rgb(255, 102, 0);
}
.wxParse-box image{
  display: block;
  width: 100%!important;
}
@import "/pages/wxParse/wxParse.wxss";

