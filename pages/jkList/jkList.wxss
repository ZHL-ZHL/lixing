/* pages/jkList/jkList.wxss */
page {
  height: 100%;
  width: 100%;
}

.nav {
  padding: 20rpx;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  
}

.nav swiper {
  height: 100%;
  overflow-y:auto;
}
.nav swiper-item{
  overflow-y:auto;
}

.iconimage {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
  border-radius: 4px;
}

.navItem {
  width: 100%;
  height: 24%;
  border-radius: 10rpx;
  position: relative;
  display: inline-block;
  text-align: center;  
}

.iconName {  
  width:280rpx;
  height: 50rpx;
  color: #fff;
  font-size:26rpx;
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  margin: auto;
}
